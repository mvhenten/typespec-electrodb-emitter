{
	"name": "typespec-electrodb-emitter",
	"version": "0.1.1",
	"type": "module",
	"main": "dist/src/index.js",
	"license": "MIT",
	"release": {
		"preset": "conventionalcommits"
	},
	"exports": {
		".": {
			"types": "./dist/src/index.d.ts",
			"default": "./dist/src/index.js",
			"typespec": "./tsp/main.tsp"
		},
		"./testing": {
			"types": "./dist/src/testing/index.d.ts",
			"default": "./dist/src/testing/index.js"
		}
	},
	"devDependencies": {
		"@babel/types": "^7.27.6",
		"@biomejs/biome": "^2.3.8",
		"@types/babel__generator": "^7.27.0",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "^8.15.0",
		"@typescript-eslint/parser": "^8.15.0",
		"conventional-changelog-conventionalcommits": "^9.0.0",
		"electrodb": "^3.5.0",
		"eslint": "^9.15.0",
		"prettier": "^3.3.3",
		"recast": "^0.23.11",
		"semantic-release": "^24.2.7",
		"tsx": "^4.21.0",
		"typescript": "^5.8.3"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"pretest": "npm run build",
		"fix": "biome check --write",
		"lint": "biome check",
		"test": "npm run test:biome && npm run test:emit && npm run test:types && npm run test:unit",
		"prepublishOnly": "npm run build",
		"test:biome": "biome check src",
		"test:emit": "tsp compile test/main.tsp --config test/tspconfig.yaml",
		"test:unit": "node --import tsx --test test/**.test.ts",
		"test:types": "tsc -p tsconfig.test.json"
	},
	"dependencies": {
		"@babel/generator": "^7.27.5",
		"@babel/parser": "^7.27.5",
		"@typespec/compiler": "^1.1.0"
	}
}
